{
  "name": "small-toolkit",
  "version": "1.0.0",
  "description": "small frame",
  "scripts": {
    "build": "npm run copy:bundle && npm run build:small && npm run build:android",
    "build:small": "android/buildSmall.sh",
    "build:android": "android/buildRelease.sh",
    "dev:android": "android/runDebug.sh",
    "serve": "node src/utils/ip && serve -p 8888",
