    "npmlog": "^4.0.0",
    "opener": "^1.4.1",
    "prompt": "^1.0.0",
    "fs-extra": "^2.0.0",
    "xtoolkit": "^0.2.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "body-parser": "^1.17.1",
    "browserify": "^13.3.0",
    "eslint": "^3.14.0",
    "express": "^4.15.2",
